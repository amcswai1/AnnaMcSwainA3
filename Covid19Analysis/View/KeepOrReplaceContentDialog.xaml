<ContentDialog
    x:Class="Covid19Analysis.View.KeepOrReplaceContentDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Title="Merging Data"
    PrimaryButtonText="Keep"
    SecondaryButtonText="Replace">
    
    <Grid>
        <CheckBox Checked="HandleAllDuplicatesCheckBox_OnChecked" Content="Do this for all duplicates" Margin="0,72,0,0" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="0,33,0,0" Text="What would you like to do with duplicate data?" TextWrapping="Wrap" VerticalAlignment="Top"/>
    </Grid>
</ContentDialog>
